{
  "name": "prisma-playground",
  "version": "0.0.0",
  "description": "prisma playground",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --no-notify --respawn src/index.ts",
    "postinstall": "npm run prisma:init",
    "prisma:init": "node scripts/initdatabase.js playground && prisma migrate dev --name init --preview-feature",
    "prisma:update": "prisma db push --preview-feature"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/cli": "^2.15.0",
    "@types/node": "^14.14.22",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "ts-node-dev": "^1.1.1"
  },
  "dependencies": {
    "@prisma/client": "^2.15.0",
    "sqlite": "^4.0.19"
  }
}
